<configs>
	<append xpath="/windows">

		<!--Do not remove or change the controller below (TabSelector)-->
		<window name="RevsWW_ServerInfo" anchor="CenterTop" pos="-450,-100" width="900" height="850" controller="TabSelector" cursor_area="true">
