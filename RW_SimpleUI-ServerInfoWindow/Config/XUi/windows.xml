						<simplebutton name="tabButton" depth="2" width="180" height="43" font_size="32" bordercolor="0,0,0,180" defaultcolor="[darkestGrey]" selectedsprite="menu_empty" selectedcolor="[mediumGrey]" foregroundlayer="false" />
					</rect>
				</grid>
			</rect>

