			<sprite depth="3" name="barBackground" pos="1,-1" color="${barBackground_color}" height="${height-2}" width="${width-2}" sprite="ui_game_filled_circle" type="sliced" />
			<sprite depth="5" name="BarContent" pos="1,-1" color="${barContent_color}" height="${height-2}" width="${width-2}" sprite="ui_game_filled_circle" type="filled" filldirection="radial360" fill="0" />
			<sprite depth="5" name="topLayer" pos="${topLayer_pos}" color="${topLayer_color}" height="${topLayer_height}" width="${topLayer_width}" sprite="ui_game_filled_circle" globalopacity="${topLayer_globalopacity}" />
			<sprite depth="7" name="Icon" atlas="{staticonatlas|once}" sprite="${sprite}" color="${icon_color}" size="${icon_size}" pos="${icon_pos}" pivot="center" foregroundlayer="true"/>
			<label depth="8" name="TextContent" pos="${text_pos}" font_size="32" color="${text_color}" justify="${text_justify}" text="${text}" width="${text_width}" height="${text_height}"/>
		</rect>
	</sui_hud_simple_radial_statBar>

	<sui_hud_simpleNoFill_round>
		<rect width="${width}" height="${height}" pos="${pos}">	
			<sprite depth="1" name="border" color="${border_color}" sprite="ui_game_filled_circle" globalopacity="${border_globalopacity}" />
			<sprite depth="2" name="background" pos="${background_pos}" width="${width-Round(border_thickness, 0)}" height="${height-Round(border_thickness, 0)}" color="${background_color}" sprite="ui_game_filled_circle" globalopacity="${background_globalopacity}" tooltip="${tooltip}" on_hover="${on_hover}" />
			<sprite depth="3" name="Icon" sprite="${sprite}" color="${icon_color}" size="${icon_size}" pos="${icon_pos}" foregroundlayer="true" />
			<label depth="4" name="Text" pos="${text_pos}" width="${text_width}" height="${text_height}" font_size="32" color="${text_color}" text="${text}" justify="${text_justify}" effect="outline" />
		</rect>
	</sui_hud_simpleNoFill_round>

	<sui_hud_buff>
		<panel width="${width}" height="${height}" pos="${pos}" controller="ActiveBuffEntry" disableautobackground="true" >	
			<sprite depth="1" name="border" color="${border_color}" type="sliced" fillcenter="false" visible="{# bufficon != ''}" />
			<sprite depth="2" name="background" color="${background_color}" type="sliced" visible="{# bufficon != ''}" />
			<sprite depth="3" name="Icon" sprite="{bufficon|once}" size="${icon_size}" pos="${icon_pos}" color="${icon_color}" foregroundlayer="true" tooltip="${tooltip}" on_hover="${on_hover}" />
			<label depth="4" name="Text" pos="${text_pos}" width="${text_width}" height="${text_height}" font_size="32" color="${text_color}" text="${text}" justify="${text_justify}" effect="outline" />
			<sprite depth="8" name="border" color="${border_color}" type="sliced" fillcenter="false" visible="{# bufficon != ''}" />
		</panel>
	</sui_hud_buff>

