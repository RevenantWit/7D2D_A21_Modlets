<configs>

<!--Add a new material. This is what the items we want to scrap will be made of.-->
<!--Forge category is the new resource item created in the items xml that we get back when scrapping.-->
<append xpath="/materials">

<material id="MresourceRWSkillPapers">
	<property name="damage_category" value="cloth"/>
	<property name="surface_category" value="cloth"/>
	<property name="forge_category" value="resourceRWSkillPapers"/>
	<property name="Experience" value="0"/>
</material>

</append>

</configs>
