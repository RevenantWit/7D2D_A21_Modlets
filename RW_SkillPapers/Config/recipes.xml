<configs>

<!--Add the new scrap recipe for the new material added.-->
<!--Recipe name is the forge category listed in the materials xml and is the new resource item created in the items xml.-->
<insertafter xpath="//recipe[@name='resourceBone']">
<recipe name="resourceRWSkillPapers" count="1" tooltip="ttScrapMetalBrass" always_unlocked="true" tags="salvageScrap"> <wildcard_forge_category /> </recipe>
</insertafter>

<!--Add the skill magazines as recipes so they can be crafted-->
<append xpath="/recipes">

